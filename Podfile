  # Pods for BaconCoders_OMG_iOS

  target 'BaconCoders_OMG_iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BaconCoders_OMG_iOSUITests' do
    # Pods for testing
  end

end
